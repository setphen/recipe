{
  "name": "@recipe-ui/legacy-theme",
  "version": "1.0.0",
  "description": "ezCater's React UI Component library - the 'standard' theme from releases prior to Recipe v11",
  "license": "MIT",
  "main": "lib/main.js",
  "module": "lib/module.js",
  "types": "lib/types.d.ts",
  "source": "src/index.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ezcater/recipe"
  },
  "dependencies": {
    "@react-types/provider": "^3.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=4.x"
      }
    },
    "module": {
      "engines": {
        "browsers": ["ie 11"]
      }
    }
  }
}
